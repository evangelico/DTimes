@font-face
{
	font-family:FontAwesome;
	src:url(../font/fontawesome-webfont.eot?#iefix&v=3.0.1) format(embedded-opentype),url(../font/fontawesome-webfont.woff?v=3.0.1) format(woff),url(../font/fontawesome-webfont.ttf?v=3.0.1) format(truetype);
	font-weight:400;
	font-style:normal;
}

*
{
	border:0;
	margin:0;
	padding:0;
}

*,:after,:before
{
	-webkit-box-sizing:border-box;
	-moz-box-sizing:border-box;
	box-sizing:border-box;
}

body,html
{
	height:100%;
	width:100%;
}

label,input,button,select,textarea
{
	font-weight:normal;
	line-height:20px;
}

input[disabled],select[disabled],textarea[disabled],input[readonly],select[readonly],textarea[readonly]
{
	cursor:not-allowed;
	background-color:#eee;
}

select
{
	height:30px;
	line-height:30px;
}

h1, h2, h3, h4, h5, h6 {
margin: 10px 0;
font-family: inherit;
font-weight: bold;
line-height: 20px;
color: inherit;
text-rendering: optimizelegibility;
}

h4 {
font-size: 17.5px;
}

.clear{
	height: 30px;
}

.alert-info {
color: #3a87ad;
background-color: #d9edf7;
border-color: #bce8f1;
}

.ui-widget
{
	font-size:.8em!important;
}

.main_container
{
	background-color:#368ee0;
	width:100%;
}

.central_container
{
	background-color:#ffffff;
	width:100%;
	min-height:720px;
}

.header
{
	width:100%;
	height:50px;
}

.header .menu
{
	width:70%;
	float:left;
}

.header .user
{
	width:20%;
	float:left;
	text-align:center;
	padding:.7% 0 .8%;
}

.header .user:hover
{
	background:#1e74c5;
}

.header .user a
{
	text-decoration:none;
	color:#ffffff;
}

.header .user p
{
	display:inline;
}

.header .user img
{
	width:30px;
	height:30px;
	vertical-align:middle;
	margin-left:5%;
}

.logo_big
{
	width:100%;
	text-align:center;
}

.logo_small
{
	width:10%;
	float:left;
}

.logo_small img
{
	width:30px;
	height:30px;
	margin:6% 0 6% 15%;
}

.logo_big img
{
	width:300px;
	height:116px;
	margin-top:2%;
}

.login_title
{
	width:100%;
	color:#ffffff;
	text-align:center;
	font-size:28px;
	margin:15px 0;
}

.login_form .form
{
	width:370px;
	background-color:#ffffff;
	height:330px;
	color:#000000;
}

.login_form .form .intro
{
	padding-left:35px;
	font-size:24px;
	text-align:left;
	padding-top:20px;
	padding-bottom:15px;
}

.login_form .form .label
{
	padding-left:35px;
	font-size:20px;
	text-align:left;
	padding-top:8px;
	padding-bottom:15px;
}

.login_form .form input[type*=text],input[type*=password]
{
	font-size:15px;
	text-align:left;
	border:1px solid #eeeeee;
	color:#000000;
	width:60%;
	margin-right:19%;
	padding:2% 1%;
}

.login_form .form input[type*=submit]
{
	background-color:#368ee0;
	color:#ffffff;
	font-size:12px;
	margin:1% 0 6% 70%;
	padding:3% 5%;
}

.login_form .form .footer
{
	background-color:#eeeeee;
	height:50px;
	position:relative;
}

.section_preview
{
	width:100%;
	height:100px;
	float:left;
}

.section_preview .title
{
	font-size:30px;
	color:#2a2a2a;
	line-height:24px;
	font-weight:400;
	float:left;
	width:18%;
	margin:37px 0 0 27px;
}

.section_preview .status
{
	float:left;
	text-align:right;
	width:80%;
}

.section_preview .box
{
	color:#ffffff;
	float:right;
	font-size:13px;
	height:61px;
	line-height:20px;
	list-style-type:none;
	margin-top:19px;
	text-align:left;
	width:200px;
	margin-right:15px;
	padding:10px;
}

.box i
{
	-webkit-font-smoothing:antialiased;
	display:block;
	float:left;
	font-family:FontAwesome;
	font-size:30px;
	font-style:normal;
	font-weight:normal;
	height:31px;
	line-height:normal;
	text-align:center;
	text-decoration:none;
	vertical-align:baseline;
	width:37.5px;
	margin:5px 0;
}

.icon-calendar:before
{
	content:"\f073";
}

.icon-money:before
{
	content:"\f0d6";
}

.box .details
{
	display:block;
	font-size:13px;
	height:41px;
	line-height:20px;
	margin-left:50px;
	text-align:left;
	width:150px;
}

.details span
{
	display:block;
}

.box .details .big
{
	font-size:16px;
}

.main_section
{
	padding-top:30px;
	float:left;
	padding-left:20px;
	padding-right:14px;
	width:100%;
}

.main_section .box_long
{
	float:left;
	font-size:13px;
	height:120px;
	line-height:20px;
	text-align:left;
	width:245px;
	color:#ffffff;
	margin:5px 0 0 5px;
}

.main_section .box_short
{
	float:left;
	font-size:13px;
	height:120px;
	line-height:20px;
	list-style-type:none;
	position:relative;
	text-align:left;
	width:120px;
	margin:5px 0 0 5px;
}

.main_section .box_long a
{
	color:#ffffff;
	width:245px;
	text-decoration:none;
	display:block;
	font-size:13px;
	height:120px;
	line-height:20px;
	text-align:center;
}

.main_section .box_short a
{
	color:#ffffff;
	width:120px;
	text-decoration:none;
	display:block;
	font-size:13px;
	height:120px;
	line-height:20px;
	text-align:center;
}

.main_section .box_long i
{
	font-family:FontAwesome;
	font-size:64px;
	font-style:normal;
	line-height:normal;
	width:100%;
	position:relative;
	top:15px;
	float:left;
}

.main_section .box_short i
{
	font-family:FontAwesome;
	font-size:48px;
	font-style:normal;
	line-height:normal;
	position:relative;
	top:25px;
	width:100%;
	float:left;
}

.main_section .box_long .name
{
	font-size:15px!important;
	padding-left:10px;
	padding-top:25px;
	float:left;
}

.main_section .box_short .name
{
	font-size:15px!important;
	padding-top:43px;
	padding-left:15px;
	float:left;
}

.icon-eye-open:before
{
	content:"\f06e";
}

.icon-cogs:before
{
	content:"\f085";
}

.icon-star:before
{
	content:"\f005";
}

.icon-shopping-cart:before
{
	content:"\f07a";
}

.icon-dashboard:before
{
	content:"\f0e4";
}

.icon-bolt:before
{
	content:"\f0e7";
}

.icon-inbox:before
{
	content:"\f01c";
}

.green
{
	background:#393;
}

.blue
{
	background:#368ee0;
}

.orange
{
	background-color:#F8A31F;
}

.red
{
	background:#e51400;
}

.lime
{
	background:#8cbf26;
}

.brown
{
	background:#a05000;
}

.footer_container
{
	width:100%;
	padding-top:2%;
	padding-bottom:2%;
	float:left;
	background:#368ee0;
}

.footer_container .footer
{
	background-color:#d9edf7;
	height:60px;
}

.footer_white
{
	background-color:#ffffff;
}

.footer_container .footer p
{
	color:#8fc0ee;
	text-align:right;
	position:relative;
	top:40px;
	right:20px;
}

.main_section .box-bordered.box-color .box-title
{
	border:2px solid #368ee0;
	background:#368ee0;
	padding:7px 0 7px 10px;
}

.main_section .box-bordered.box-color .box-title h3
{
	line-height:24px;
	font-weight:400;
	color:#ffffff;
	font-size:20px;
	margin:3px 0;
}

.main_section .box-bordered.box-color .box-title h3 i
{
	font-family:FontAwesome;
	font-style:normal;
	vertical-align:baseline;
	margin:0 10px;
}

.icon-th-list:before
{
	content:"\f00b";
}

.main_section .box-bordered.box-color .box-content
{
	border:2px solid #368ee0;
	border-top:0;
}

.nopadding
{
	padding:0!important;
}

.control-group
{
	border-bottom:1px solid #ddd;
	background:#fff;
	margin:0;
	padding:0;
}

.control-group .control-label
{
	color:#555;
	width:160px;
	display:inline-block;
	margin:0;
	padding:15px 10px 5px;
}

.control-group .controls
{
	display:inline-block;
	padding:10px;
}

.uneditable-input
{
	border:1px solid #ccc;
}

.control-group .controls input[type=text],input[type=password],input[type=datetime],input[type=datetime-local],input[type=date],input[type=month],input[type=time],input[type=week],input[type=number],input[type=email],input[type=url],input[type=search],input[type=tel],input[type=color],.uneditable-input
{
	height:20px;
	font-size:14px;
	color:#555;
	padding:15px 6px;
}

.control-group .controls select
{
	width:220px;
	background-color:#fff;
	border:1px solid #ccc;
}

.control-group .paid{
	color: #555;
margin: 0;
padding: 15px 10px 5px;
	
}

.input-xlarge
{
	width:270px;
	border:1px solid #ccc;
}

.textfieldRequiredMsg,.selectRequiredMsg
{
	display:none;
	color:#CC3333;
	border:1px solid #CC3333;
}

.input-medium
{
	width:150px;
	border:1px solid #ccc;
}

.input-large
{
	width:210px;
	border:1px solid #ccc;
}

.input-small{
	border:1px solid #ccc;
	width: 90px;
	
	height: 20px;
padding: 15px 6px;
font-size: 14px;
line-height: 20px;
color: #555;
}

form
{
	margin: 0 0 10px;
}

.form-actions
{
	background:0;
	border:0;
	margin-top:20px;
	margin-bottom:20px;
	padding:19px 20px 20px 180px;
}

.btn
{
	border:0;
	color:#444;
	padding:5px 9px;
}

.btn.btn-primary
{
	background:#368ee0;
	color:#fff;
	cursor:pointer;
	padding:5px 9px;
}

.btn.btn-info
{
	background:#a4ccf1;
	cursor:pointer;
}

.btn.btn-warning
{
	background:#fab750;
	cursor:pointer;
}

.btn.btn-danger
{
	background:#f43;
	color:#fff;
	cursor:pointer;
}

.dataTables_wrapper .DTTT_container
{
	margin:10px 5px 10px 0;
}

div.DTTT_container
{
	position:relative;
	float:right;
	margin-bottom:1em;
}

button.DTTT_button,div.DTTT_button,a.DTTT_button
{
	position:relative;
	float:left;
	cursor:pointer;
	font-size:.88em;
	color:black!important;
	padding:5px 8px;
}

.dataTables_wrapper .DTTT_container .DTTT_button
{
	background:#eee;
	box-shadow:none;
	border-radius:0;
	border:0;
	margin-right:5px;
}

.dataTables_wrapper .DTTT_container .DTTT_button:hover
{
	background:#ddd;
}

.dataTables_wrapper .dataTables_filter
{
	float:right;
	margin:10px 10px 5px 5px;
}

.dataTables_wrapper .dataTables_filter input[type=text]
{
	background-color:#fff;
	border:1px solid #ccc;
	display:inline-block;
	height:20px;
	font-size:14px;
	line-height:20px;
	color:#555;
	vertical-align:middle;
	padding:13px 6px;
}

table
{
	max-width:100%;
	background-color:transparent;
	border-spacing:0;
}

.table th,.table td
{
	line-height:20px;
	text-align:left;
	vertical-align:top;
}

.table th
{
	font-weight:bold;
}

.table tr th,.table tr td
{
	border-top:1px dotted #ddd;
	padding:10px;
}

.table tr:hover td
{
	background:#f8f8f8;
}

.table-bordered
{
	border:1px solid #ddd;
	border-collapse:separate;
	border-left:0;
}

.table-bordered th,.table-bordered td
{
	border-left:1px solid #ddd;
}

.table-bordered.dataTable
{
	border-top:1px solid #ddd;
}

.main_section .box-bordered .table
{
	margin-bottom:0;
}

.main_section .box-bordered .table.table-bordered
{
	border-right:0;
}

.table.dataTable .sorting
{
	padding-right:30px;
	white-space:nowrap;
	background:#eee;
}

.dataTables_wrapper .dataTables_info {
margin-left: 10px;
margin-top: 10px;
margin-bottom: 10px;
display: inline-block;
}


.dataTables_wrapper .dataTables_paginate {
margin-right: 5px;
margin-top: 10px;
margin-bottom: 10px;
display: inline-block;
float: right;
}

.dataTables_wrapper .dataTables_paginate>a {
margin-right: 5px;
background: #eee;
padding: 3px 8px;
color: #333;
text-decoration: none;
cursor: pointer;
}


.dataTables_wrapper .dataTables_paginate>span>a {
padding: 3px 8px;
margin-right: 5px;
background: #eee;
color: #333;
text-decoration: none;
cursor: pointer;
}

.dataTables_wrapper .dataTables_paginate>span>a.paginate_active {
background: #368ee0;
color: #fff;
}

.table-bordered thead:first-child tr:first-child th,.table-bordered tbody:first-child tr:first-child th,.table-bordered tbody:first-child tr:first-child td
{
	border-top:0;
}

.main_section .box-bordered .table.table-bordered th:first-child,.main_section .box-bordered .table.table-bordered td:first-child
{
	border-left:0;
}

.login_form,.main_section .box-bordered.box-color,.table
{
	width:100%;
}